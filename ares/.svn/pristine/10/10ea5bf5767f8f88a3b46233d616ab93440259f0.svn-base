INSERT INTO `cms_disc` VALUES (4, '在开发智能服装方面，东丽和NTT领先于其他竞争企业，2014年与高得运(Goldwin)合作推出了可测量心率的智能服装。2016年8月还开始提供远程信息服务，用户可利用智能手机等在其他场合掌握工厂等的工人的身体健康状况。', '2016-10-13 20:36:27', 4, '想象力统治世界');
